	"song" : {
	  "duration": 88,
	  "name": "Demon Slayer OP2 - Zankyou Sanka",
	  "owner": "Aimer",
	  "sheet": ["s", "l"],
	  "lyrics": [
		 { "upperText": "...Intro...", "lowerText": "Kimetsu no Yaiba", "delay": 1.0 },
		 { "upperText": "Ta ga sode ni saku genka", "lowerText": "Tada soko ni ai wo otoshita", "delay": 20.5 },
		 { "upperText": "Hade ni iro wo tokasu you ni", "lowerText": "Ginshu no tsuki wo soete", "delay": 26.0 },
		 { "upperText": "Korogaru you ni kaze wo kitte", "lowerText": "Tsumazuku goto ni tsuyoku natta", "delay": 31.0 },
		 { "upperText": "Hikari mo itami mo ikari mo", "lowerText": "Zenbu dakishimete", "delay": 37.0 },
		 { "upperText": "Erabarenakereba erabeba ii", "lowerText": "***", "delay": 42.0 },
		 { "upperText": "Koe wo karashite moeru hana no you ni", "lowerText": "Yamima wo terashitara", "delay": 47.5 },
		 { "upperText": "Aimai sugiru seikai mo fumen ni shite", "lowerText": "Yoru wo kazoete asa wo egaku you na", "delay": 54.0 },
		 { "upperText": "Azayaka na ne wo narasu", "lowerText": "Donna ni fukai koukai mo", "delay": 62.0 },
		 { "upperText": "Donna ni takai genkai mo", "lowerText": "Kakikeshite zankyou", "delay": 67.5 },
		 { "upperText": "", "lowerText": "", "delay": 79.0 }
	  ]
	}
